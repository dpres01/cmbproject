					<i id="respusericnop" class="fa fa-user-o"></i>
					<i id="respusericncl" class="fa fa-times" style="display:none;"></i>
				</div>
				<div id="respmn" style="float:left;padding-top:2px;cursor:pointer;width:15px;">
					<i id="respmnicnop" class="fa fa-bars"></i>
					<i id="respmnicncl" class="fa fa-times" style="display:none;"></i>
				</div>
				<div style="clear:both;"></div>
			</div>
			<div style="clear:both;"></div>
		</div>
		<div id="resp-menu" class="hd-resp-menu" style="">
			<div style="">Déposer une annonce</div>
			<div style="hd-resp-menu-elm">Offres</div>
			<div style="hd-resp-menu-elm">Demandes</div>
			<div style="hd-resp-menu-elm">Mes favoris</div>
			<div style="hd-resp-menu-elm">Boutiques</div>
			<div style="hd-resp-menu-elm">Messages</div>
		</div>
